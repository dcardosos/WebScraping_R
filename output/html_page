<script>
	(function($) {
				
		var bindTreeSelect = function() {
			var options = {
				campoId: 'classe',
				treeSelectId: 'treeSelectClasse',
				url: '/cjpg/classeTreeSelect.do',
				processandoImgUrl: '/cjpg/imagens/spw/processandoNovo.gif',
				width: 600,
				expectedHeight: 400,
				reload: 'false',
				mostrarBotoesSelecaoRapida: true
			};

			$.saj.tree.updateSelectionText(options);
			
			$('#botaoProcurar_classe').bind('click', options, $.saj.tree.onPopupEvent);
			$('#botaoLimpar_classe').bind('click', options, $.saj.tree.onClearSelection);
			
			
		};

		bindTreeSelect();

		
		
	})(jQuery);
